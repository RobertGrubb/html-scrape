  "homepage": "https://github.com/RobertGrubb/html-scrape",
  "devDependencies": {
    "babel-cli": "^6.5.1",
    "babel-core": "^6.5.2",
    "babel-preset-es2015": "^6.5.0",
    "babel-preset-stage-2": "^6.5.0",
    "rimraf": "^2.5.2"
  },
  "files": [
    "lib/"
  ],
  "dependencies": {
    "lodash": "^4.5.1",
